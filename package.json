{
  "name": "7pret",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "start": "vite preview --host --port ${PORT:-4173}",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') require('child_process').execSync('lefthook install', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "better-auth": "^1.4.18",
    "pg": "^8.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.2.3",
    "lefthook": "^2.0.16",
    "prisma": "^7.3.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
